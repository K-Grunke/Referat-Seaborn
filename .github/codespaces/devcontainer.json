{
  "name": "Seaborn Referat",
  "image": "mcr.microsoft.com/devcontainers/python:3.13",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.13"
    }
  },
  "postCreateCommand": "pip install --upgrade pip && pip install -r requirements.txt && pip install jupyter nbconvert matplotlib pandas seaborn numpy",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-python.vscode-pylance",
        "VisualStudioExptTeam.vscodeintellicode"
      ]
    }
  },
  "remoteUser": "vscode"
}
